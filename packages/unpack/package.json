{
  "name": "@capsizecss/unpack",
  "version": "4.0.0",
  "description": "Unpack capsize font metrics from fonts",
  "keywords": [
    "capsize",
    "leading trim",
    "white space",
    "baseline",
    "basekick",
    "cap height",
    "typography",
    "line gap",
    "leading"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/capsize.git",
    "directory": "packages/unpack"
  },
  "license": "MIT",
  "author": {
    "name": "Michael Taranto",
    "homepage": "https://github.com/michaeltaranto"
  },
  "type": "module",
  "exports": {
    ".": {
      "@capsizecss/src": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./fs": {
      "@capsizecss/src": "./src/fs.ts",
      "default": "./dist/fs.mjs"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "generate": "tsx scripts/generate-weightings"
  },
  "dependencies": {
    "fontkitten": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.18.8",
    "fast-xml-parser": "^4.3.2",
    "sort-keys": "^5.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./fs": "./dist/fs.mjs",
      "./package.json": "./package.json"
    }
  },
  "main": "./dist/index.mjs"
}
