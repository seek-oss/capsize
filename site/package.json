{
  "name": "site",
  "private": true,
  "description": "Capsize site",
  "version": "0.0.0",
  "scripts": {
    "clean": "gatsby clean",
    "start": "gatsby develop -H 0.0.0.0",
    "start:vite": "vite",
    "serve": "gatsby serve --prefix-paths -H 0.0.0.0",
    "build": "gatsby build --prefix-paths",
    "build:vite": "vite build",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "deploy": "node scripts/deploy.js",
    "deploy-preview": "gatsby build && surge -p ./public -d capsize--${GITHUB_SHA}.surge.sh && node scripts/postCommitStatus.js"
  },
  "author": {
    "name": "Michael Taranto",
    "homepage": "https://github.com/michaeltaranto"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/capsize/site"
  },
  "dependencies": {
    "@capsizecss/core": "^3.1.1",
    "@capsizecss/metrics": "^1.3.0",
    "@capsizecss/unpack": "^1.0.0",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "debounce": "^1.2.0",
    "detect-font": "^0.1.5",
    "downshift": "^5.4.2",
    "hex-rgb": "^4.1.0",
    "match-sorter": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-syntax-highlighter": "^12.2.1",
    "react-use": "^15.3.2"
  },
  "devDependencies": {
    "@octokit/rest": "^18.0.3",
    "@remix-run/dev": "^2.5.1",
    "@types/debounce": "^1.2.0",
    "@types/match-sorter": "^4.0.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-helmet": "^6.0.0",
    "@types/react-syntax-highlighter": "^11.0.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "gh-pages": "^3.1.0",
    "sharp": "^0.33.2",
    "surge": "^0.21.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-node-polyfills": "^0.19.0"
  }
}
