{
  "name": "site",
  "private": true,
  "description": "Capsize site",
  "version": "0.0.0",
  "scripts": {
    "clean": "gatsby clean",
    "start": "gatsby develop -H 0.0.0.0",
    "serve": "gatsby serve --prefix-paths -H 0.0.0.0",
    "build": "gatsby build --prefix-paths",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "deploy": "node scripts/deploy.js",
    "deploy-preview": "gatsby build && surge -p ./public -d capsize--${GITHUB_SHA}.surge.sh && node scripts/postCommitStatus.js"
  },
  "author": {
    "name": "Michael Taranto",
    "homepage": "https://github.com/michaeltaranto"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/capsize/site"
  },
  "dependencies": {
    "@capsizecss/core": "^3.0.0",
    "@capsizecss/metrics": "^0.0.1",
    "@capsizecss/unpack": "^0.0.1",
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "debounce": "^1.2.0",
    "downshift": "^5.4.2",
    "emotion-theming": "^10.0.27",
    "hex-rgb": "^4.1.0",
    "match-sorter": "^4.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-syntax-highlighter": "^12.2.1",
    "react-use": "^15.3.2"
  },
  "devDependencies": {
    "@octokit/rest": "^18.0.3",
    "@types/debounce": "^1.2.0",
    "@types/match-sorter": "^4.0.0",
    "@types/react-helmet": "^6.0.0",
    "gatsby": "^2.24.12",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-offline": "^3.2.21",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.23",
    "gatsby-source-filesystem": "^2.3.23",
    "gatsby-transformer-sharp": "^2.5.11",
    "gh-pages": "^3.1.0",
    "surge": "^0.21.6",
    "transform-loader": "^0.2.4"
  }
}
